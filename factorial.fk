funk factorial1 (n: i32): i32 {
    var result: i32 = 1;
    while (n > 1) {
        result = result * n;
        n = n - 1;
    }
    return result;
}


funk factorial2 (n: i64): i64 {
    if (n == 0) {
        return 1;
    }
    return n * factorial2(n - 1);
}

funk test (n: i64): i32 {
    var a: i32[5] = [10, 20, 30];
    a[2] = a[2] + 2;
    return a[2];
}

factorial1(10);
factorial2(10);
test(10);